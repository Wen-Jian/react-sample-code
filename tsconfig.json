{
  "extends": "../../tsconfig.base.json",
  "types": [
    "react/next"
  ],
  "compilerOptions": {
    "module": "ES2022",
    "target": "es6",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "jsx": "react",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "moduleResolution": "node",
    "lib": [
